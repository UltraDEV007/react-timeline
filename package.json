{
  "name": "react-component-boilerplate",
  "description": "Boilerplate for React.js components",
  "author": "Juho Vepsalainen <bebraw@gmail.com>",
  "user": "bebraw",
  "version": "0.0.0",
  "scripts": {
    "start": "node dev-server/server.js",
    "test": "jest && npm run lint",
    "gh-pages": "webpack --config ./config/webpack.gh-pages.js",
    "deploy-gh-pages": "node ./config/deploy-gh-pages.js",
    "dist": "webpack --config ./config/webpack.dist.js && webpack --config ./config/webpack.dist.min.js && babel ./lib --out-dir dist-modules",
    "lint": "eslint . --ext .js --ext .jsx",
    "replace-meta": "node scripts/replace_meta.js"
  },
  "main": "dist-modules/index.js",
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.1.10",
    "babel-eslint": "^3.0.1",
    "babel-loader": "^5.0.0",
    "css-loader": "^0.10.1",
    "eslint": "^0.19.0",
    "eslint-loader": "^0.10.0",
    "eslint-plugin-react": "^2.1.0",
    "file-loader": "^0.8.1",
    "gh-pages": "^0.2.0",
    "highlight-loader": "^0.3.0",
    "highlight.js": "^8.5.0",
    "html-loader": "^0.2.3",
    "html-webpack-plugin": "^1.1.0",
    "jest-cli": "^0.4.0",
    "json-loader": "^0.5.1",
    "lodash": "^3.6.0",
    "markdown-loader": "^0.1.2",
    "prompt": "^0.2.14",
    "purecss": "^0.6.0",
    "react": "^0.13.1",
    "react-ghfork": "^0.3.0",
    "react-hot-loader": "^1.2.5",
    "react-tools": "^0.13.1",
    "replace": "^0.3.0",
    "style-loader": "^0.10.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.4",
    "webpack-dev-server": "^1.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bebraw/react-component-boilerplate.git"
  },
  "homepage": "https://github.com/bebraw/react-component-boilerplate",
  "bugs": {
    "url": "https://github.com/bebraw/react-component-boilerplate/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/bebraw/react-component-boilerplate/blob/master/LICENSE"
    }
  ],
  "jest": {
    "scriptPreprocessor": "./config/preprocessor.js",
    "unmockedModulePathPatterns": [
      "./node_modules/react"
    ]
  }
}
