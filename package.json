{
  "name": "react-component-boilerplate",
  "description": "Boilerplate for React.js components",
  "author": "Juho Vepsalainen",
  "user": "survivejs",
  "version": "0.6.0",
  "scripts": {
    "start": "webpack-dev-server",
    "gh-pages": "webpack",
    "deploy-gh-pages": "node ./lib/deploy_gh_pages.js",
    "dist-all": "npm run dist && npm run dist-min",
    "dist": "webpack",
    "dist-min": "webpack",
    "dist-modules": "babel ./src --out-dir ./dist-modules",
    "lint": "eslint . --ext .js --ext .jsx",
    "replace-meta": "node lib/replace_meta.js",
    "preversion": "npm run test && npm run dist-all && npm run dist-modules && git commit --allow-empty -am \"Update dist\"",
    "postpublish": "npm run gh-pages && npm run deploy-gh-pages"
  },
  "main": "dist-modules/index.js",
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.16.0",
    "eslint": "^1.3.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.3.1",
    "esprima-fb": "^15001.1001.0-dev-harmony-fb",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.3.1",
    "git-prepush-hook": "^1.0.1",
    "highlight.js": "^8.7.0",
    "html-webpack-plugin": "^1.6.1",
    "json-loader": "^0.5.2",
    "lodash": "^3.10.1",
    "markdown-to-react-components": "bebraw/markdown-to-react-components#peerdeps",
    "node-libs-browser": "^0.5.2",
    "prompt": "^0.2.14",
    "purecss": "^0.6.0",
    "react-ghfork": "^0.3.2",
    "react-hot-loader": "^1.3.0",
    "replace": "^0.3.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1",
    "webpack-merge": "^0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/survivejs/react-component-boilerplate.git"
  },
  "homepage": "https://survivejs.github.io/react-component-boilerplate/",
  "bugs": {
    "url": "https://github.com/survivejs/react-component-boilerplate/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate"
  ],
  "license": "MIT",
  "pre-push": []
}
